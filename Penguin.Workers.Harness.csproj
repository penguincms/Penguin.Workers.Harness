<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="Penguin.Projects.Shared\Penguin.Projects.Shared.csproj" />
  <PropertyGroup>
    <PackageProjectUrl>https://github.com/penguincms/Penguin.Workers.Harness</PackageProjectUrl>
    <Version>2.0.1</Version>
    <Description>Provides a shared class to be referenced by EXE files used to run implementations of the IWorker class.</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Penguin.Workers.Harness</AssemblyName>
    <RootNamespace>Penguin.Workers.Harness</RootNamespace>
    <DocumentationFile>Penguin.Workers.Harness.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Penguin.Messaging.Logging" Version="3.0.1-T9UW4G4" />
    <PackageReference Include="Penguin.Workers.Abstractions" Version="2.0.1-T9UW4G4" />
    <PackageReference Include="Penguin.Configuration" Version="2.0.2-T9UW4G4" />
    <PackageReference Include="Penguin.DependencyInjection" Version="4.1.11-T9UW4G4" />
  </ItemGroup>
</Project>